#######################
# nntrainer conf file #
#######################

# default plugin paths, below path is searched and registered when the library is being loaded.
[plugins]
# path to search for layers
layer=@PLUGIN_INSTALL_PREFIX@/layers

[fsu]
# enable memory fsu feature
fsu = @FLASH_STORAGE_UTILIZATION@

# path to save fsu file
fsu_path = @FLASH_STORAGE_UTILIZATION_PATH@

# look ahead window size
fsu_lookahead = @FLASH_STORAGE_UTILIZATION_LOOKAHEAD@
